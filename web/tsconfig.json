{
  "extends": "astro/tsconfigs/strictest",
  "baseUrl": ".",
  "paths": {
    "@components/*": [
      "src/components/*"
    ],
    "@layouts/*": [
      "src/layouts/*"
    ]
  },
  "exclude": [
    "tests",
    "tests-examples",
    "playwright.config.ts"
  ],
  "compilerOptions": {
    "jsx": "preserve"
  }
}
